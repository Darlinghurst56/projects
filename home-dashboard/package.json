{
  "name": "home-dashboard",
  "version": "1.0.0",
  "description": "Unified Home Dashboard with Google Services, DNS Monitoring, and AI Integration",
  "main": "server/index.js",
  "scripts": {
    "start": "node --max-old-space-size=3072 server/index.js",
    "start:debug": "node --max-old-space-size=3072 server/crash-debug.js",
    "start:safe": "node --max-old-space-size=3072 server/crash-debug.js",
    "dev": "concurrently --max-processes=3 --restart-tries=3 --restart-after=2000 \"npm run server:dev\" \"npm run client:dev\"",
    "dev:debug": "concurrently --max-processes=4 --restart-tries=3 --restart-after=2000 \"npm run server:dev\" \"npm run client:dev\" \"npm run browser:debug\"",
    "dev:performance": "concurrently --max-processes=4 --restart-tries=3 --restart-after=2000 \"npm run server:dev:optimized\" \"npm run client:dev:optimized\" \"npm run monitor:performance\"",
    "browser:debug": "sleep 5 && ./chrome/linux-138.0.7204.168/chrome-linux64/chrome --remote-debugging-port=9222 --no-sandbox --disable-setuid-sandbox --memory-pressure-off --max_old_space_size=1024 http://localhost:3003",
    "server:dev": "NODE_OPTIONS=\"--max-old-space-size=2048\" nodemon --watch server --ext js,json --delay 1000ms server/index.js",
    "server:dev:optimized": "NODE_OPTIONS=\"--max-old-space-size=2048\" nodemon --watch server --ext js,json --delay 800ms --inspect=0.0.0.0:9229 server/index.js",
    "client:dev": "vite --host 0.0.0.0 --port 3003",
    "client:dev:optimized": "NODE_OPTIONS=\"--max-old-space-size=1024\" vite --host 0.0.0.0 --port 3003 --force",
    "monitor:performance": "node scripts/performance-monitor.js",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit --verbose",
    "test:integration": "jest --testPathPattern=integration --verbose",
    "test:e2e": "playwright test --reporter=line",
    "test:e2e:headed": "playwright test --headed --reporter=line",
    "test:e2e:ui": "playwright test --ui",
    "test:debug": "playwright test --headed --debug",
    "test:inspect": "PWDEBUG=1 playwright test",
    "test:devtools": "playwright test --headed --project=chromium-debug --reporter=line",
    "test:visual": "playwright test --update-snapshots",
    "test:live": "node validate-dashboard.js",
    "test:ux": "playwright test tests/e2e/live-ux-validation.spec.js --reporter=line",
    "test:a11y": "playwright test tests/accessibility/ --reporter=line",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run lint && npm run test:unit && npm run test:integration && npm run build && npm run test:e2e",
    "test:mvp": "npm run lint && npm run test:unit && npm run build && npm run test:live && npm run test:ux && npm audit --audit-level=moderate",
    "test:quick": "npm run lint && npm run test:unit",
    "test:security": "npm audit --audit-level=moderate",
    "lint": "eslint src/ server/ --ext .js,.jsx",
    "lint:fix": "eslint src/ server/ --ext .js,.jsx --fix",
    "docs:validate": "node scripts/validate-documentation.js",
    "docs:validate:strict": "node scripts/validate-documentation.js --threshold=85",
    "pre-commit": "npm run docs:validate --threshold=75 && npm run lint",
    "setup": "node scripts/setup.js",
    "install-dashy": "npm install -g dashy",
    "bridge:start": "node server/integrations/bridge.js",
    "bridge:dev": "nodemon server/integrations/bridge.js"
  },
  "keywords": [
    "dashboard",
    "home-automation",
    "google-api",
    "dns-monitoring",
    "ai-integration",
    "dashy",
    "family-dashboard"
  ],
  "author": "HouseAI Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "framer-motion": "^10.16.0",
    "googleapis": "^150.0.1",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "styled-components": "^6.0.0",
    "turndown": "^7.2.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "xss": "^1.0.15",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "axe-playwright": "^1.2.3",
    "babel-jest": "^30.0.5",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "eslint-plugin-security": "^1.7.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "ora": "^5.4.1",
    "puppeteer": "^24.16.0",
    "puppeteer-core": "^24.15.0",
    "vite": "^4.4.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "../../../"
  },
  "bugs": {
    "url": "../../../issues"
  },
  "homepage": "../../../README.md"
}
