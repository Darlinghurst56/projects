# HTTP/API Client Environment Variables
# Copy this file to .env and fill in your API keys

# Weather API (OpenWeatherMap)
WEATHERAPI_API_KEY=your_openweathermap_api_key_here

# News API
NEWSAPI_API_KEY=your_newsapi_api_key_here

# Google API (REQUIRED for TaskMaster MCP integration)
GOOGLE_API_KEY=your_google_api_key_here

# GitHub API (REQUIRED for GitHub MCP server)
GITHUB_TOKEN=your_github_personal_access_token_here

# Other API Keys (add as needed)
# CUSTOM_API_KEY=your_custom_api_key_here

# HTTP Client Configuration
HTTP_CLIENT_LOG_LEVEL=info
HTTP_CLIENT_CACHE_ENABLED=true
HTTP_CLIENT_RETRY_ENABLED=true

# MCP Server Configuration
MCP_LOG_LEVEL=info
MCP_TIMEOUT=30000

# Dashboard Configuration
DASHBOARD_BASE_URL=http://localhost:3000
DASHBOARD_API_PREFIX=/api/v1

# Development Settings
NODE_ENV=development
DEBUG=claude-project:*

# Security Configuration
SESSION_SECRET=your_session_secret_here
JWT_SECRET=your_jwt_secret_here

# TaskMaster AI Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
XAI_API_KEY=your_xai_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
OLLAMA_API_KEY=your_ollama_api_key_here