# LiteLLM Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Configuration
GOOGLE_API_KEY=your_google_api_key_here

# TaskMaster Integration
TASKMASTER_API_URL=http://host.docker.internal:3001
TASKMASTER_INTEGRATION_ENABLED=true

# LiteLLM Configuration
LITELLM_CONFIG_PATH=/home/darlinghurstlinux/shared/config/litellm/litellm-config.yaml
LITELLM_LOG_LEVEL=INFO
LITELLM_PORT=4000
LITELLM_MASTER_KEY=taskmaster-litellm-key

# Redis Configuration (optional, for caching)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Security
LITELLM_MASTER_KEY=taskmaster-litellm-key

# Development/Debug
DEBUG=false
LITELLM_DEBUG=false