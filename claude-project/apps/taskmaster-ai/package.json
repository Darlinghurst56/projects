{
  "name": "taskmaster-ai",
  "version": "1.1.0",
  "description": "Multi-agent coordination, agent registry, and task assignment.",
  "main": "app/core/taskmaster-api-server.js",
  "scripts": {
    "start": "node app/core/taskmaster-api-server.js",
    "start-web": "node scripts/start-web.js",
    "system:start": "npm run start &",
    "system:stop": "pkill -f taskmaster-api-server.js",
    "system:status": "curl -s http://localhost:3010/api/health | jq '.'",
    "cleanup": "node app/core/task-cleanup-jobs.js --start",
    "cleanup:status": "node app/core/task-cleanup-jobs.js --status",
    "cleanup:force": "node app/core/task-cleanup-jobs.js --force-all",
    "wrapper:help": "node app/core/task-update-wrapper.js --help",
    "agents:start": "node app/core/simple-agent-manager.js start-all",
    "agents:stop": "node app/core/simple-agent-manager.js stop-all",
    "agents:status": "node app/core/simple-agent-manager.js status",
    "agents:health": "curl -s http://localhost:3010/api/agents | jq '.totalAgents,.activeAgents'",
    "agents:batch-start": "node app/core/simple-agent-manager.js start-batch",
    "agents:batch-stop": "node app/core/simple-agent-manager.js stop-batch",
    "agents:health-check": "node app/core/simple-agent-manager.js health-check",
    "agents:system-status": "node app/core/simple-agent-manager.js system-status",
    "agents:queue-task": "node app/core/simple-agent-manager.js queue-task",
    "agents:process-queue": "node app/core/simple-agent-manager.js process-queue",
    "agents:cleanup": "node app/core/simple-agent-manager.js cleanup",
    "agents:auto-assign": "node app/core/simple-agent-manager.js auto-assign",
    "agents:analyze-task": "node app/core/simple-agent-manager.js analyze-task",
    "agents:capability-report": "node app/core/simple-agent-manager.js capability-report",
    "agents:smart-process-queue": "node app/core/simple-agent-manager.js smart-process-queue",
    "agents:get-recommendations": "node app/core/simple-agent-manager.js get-recommendations",
    "qa-agent": "node app/agents/qa-specialist-agent.js",
    "backend-agent": "node app/agents/backend-agent.js",
    "frontend-agent": "node app/agents/frontend-agent.js",
    "devops-agent": "node app/agents/devops-agent.js",
    "orchestrator-agent": "node app/agents/orchestrator-agent.js",
    "developer-interface": "node app/developer-interface/cli/task-router.js",
    "task-router": "node app/developer-interface/cli/task-router.js",
    "agent-manager": "node app/developer-interface/cli/agent-manager.js",
    "dashboard": "node app/cli/main-dashboard.js",
    "overview": "node app/cli/agent-overview.js",
    "details": "node app/cli/agent-details.js",
    "workflow": "node app/cli/workflow-visualizer.js",
    "tasks:status": "curl -s http://localhost:3010/api/tasks | jq '.metadata.total'",
    "batch:quick-start": "node app/cli/batch-operations.js quick-start",
    "batch:quick-shutdown": "node app/cli/batch-operations.js quick-shutdown",
    "batch:health-check": "node app/cli/batch-operations.js health-check",
    "batch:restart": "node app/cli/batch-operations.js restart",
    "batch:dev-workflow": "node app/cli/batch-operations.js dev-workflow",
    "batch:cleanup": "node app/cli/batch-operations.js cleanup",
    "workflow:run": "node app/cli/workflow-manager.js run",
    "workflow:list": "node app/cli/workflow-manager.js list",
    "workflow:show": "node app/cli/workflow-manager.js show",
    "workflow:morning": "node app/cli/workflow-manager.js run morning-startup",
    "workflow:dev": "node app/cli/workflow-manager.js run development-session",
    "workflow:evening": "node app/cli/workflow-manager.js run evening-shutdown",
    "workflow:test": "node app/cli/workflow-manager.js run quick-test",
    "validate": "node setup-validation.js",
    "setup": "npm install && node setup-validation.js",
    "test": "echo 'Running TaskMaster coordination tests...' && node .taskmaster/agents/__tests__/run-tests.cjs",
    "test:ui": "node tests/integration/test-tasks-web-interface.js",
    "test:all": "npm run test && npm run test:ui",
    "lint": "echo 'Linting with ESLint...' && npx eslint . --ext .js,.cjs --ignore-pattern node_modules/",
    "test:ui:playwright": "npx playwright test tests/ui/ --timeout=30000",
    "test:ui:headed": "npx playwright test tests/ui/ --headed --timeout=30000",
    "telemetry:dashboard": "node .taskmaster/agents/telemetry-dashboard.js",
    "telemetry:report": "node .taskmaster/agents/agent-telemetry-tracker.js report",
    "telemetry:live": "node .taskmaster/agents/telemetry-dashboard.js live",
    "telemetry:export": "node .taskmaster/agents/telemetry-dashboard.js export"
  },
  "keywords": [
    "taskmaster",
    "multi-agent",
    "coordination",
    "ai",
    "automation"
  ],
  "author": "TaskMaster AI Team",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.54.1",
    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "inquirer": "^9.3.7",
    "node-fetch": "^2.7.0",
    "ora": "^8.2.0",
    "playwright": "^1.54.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "eslint": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}